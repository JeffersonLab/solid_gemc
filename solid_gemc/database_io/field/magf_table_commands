use user_geometry_1;
drop table if exists magnetic_fields;
create table magnetic_fields (
name VARCHAR(40) UNIQUE,
type VARCHAR(150),
magnitude VARCHAR(240),
swim_method VARCHAR(40),
description VARCHAR(100),
PRIMARY KEY (`name`)
);
LOAD DATA LOCAL INFILE 'magnetic_fields.txt' into TABLE magnetic_fields;
select name,type,magnitude,swim_method,description from magnetic_fields;
