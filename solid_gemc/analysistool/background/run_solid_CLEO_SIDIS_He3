#!/bin/sh
gemc \
-gcard=solid_CLEO_SIDIS_He3.gcard \
-DBHOST=$GEMC_HOST \
-HIT_PROCESS_LIST=solid \
-HALL_MATERIAL=Air \
-USE_PHYSICSL=QGSP_BERT \
-RECORD_PASSBY=1 \
-FIELD_DIR=$GEMC_field \
-HALL_FIELD=solenoid_CLEO \
-BEAM_P="e-, 11*GeV, 0*deg, 0*deg" \
-BEAM_V="(0, 0, -400)cm" \
-SPREAD_V="(0.21, 0)cm" \
-N=1e6
-OUTPUT="evio,out.evio"
-USE_QT=0

