use user_banks;

drop table if exists user_banks.EC;
create table user_banks.EC( name VARCHAR(40));
alter table EC add column id int;
alter table EC add column type int;
alter table EC add column activated int;
alter table EC add column description VARCHAR(100);
LOAD DATA LOCAL INFILE 'bankdefs/EC.bank' into TABLE EC;


drop table if exists user_banks.FLUX;
create table user_banks.FLUX( name VARCHAR(40));
alter table FLUX add column id int;
alter table FLUX add column type int;
alter table FLUX add column activated int;
alter table FLUX add column description VARCHAR(100);
LOAD DATA LOCAL INFILE 'bankdefs/FLUX.bank' into TABLE FLUX;


drop table if exists user_banks.Generated;
create table user_banks.Generated( name VARCHAR(40));
alter table Generated add column id int;
alter table Generated add column type int;
alter table Generated add column activated int;
alter table Generated add column description VARCHAR(100);
LOAD DATA LOCAL INFILE 'bankdefs/Generated.bank' into TABLE Generated;


drop table if exists user_banks.raw;
create table user_banks.raw( name VARCHAR(40));
alter table raw add column id int;
alter table raw add column type int;
alter table raw add column activated int;
alter table raw add column description VARCHAR(100);
LOAD DATA LOCAL INFILE 'bankdefs/raw.bank' into TABLE raw;


drop table if exists user_banks.TRACE;
